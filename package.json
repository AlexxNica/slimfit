{
  "name": "slimfit",
  "version": "0.0.0",
  "description": "Slim library for fitting things",
  "author": "Krist Wongsuphasawat <krist.wongz@gmail.com> (http://kristw.yellowpigz.com)",
  "keywords": [],
  "engines": {
    "node": "~6.3"
  },
  "license": "Apache-2.0",
  "main": "dist/slimfit.min.js",
  "files": [
    "src/**/*.*",
    "dist/*.*"
  ],
  "devDependencies": {
    "babel-core": "^6.3.21",
    "babel-loader": "^6.2.0",
    "babel-plugin-transform-object-assign": "^6.1.12",
    "babel-preset-es2015": "^6.3.13",
    "bower": "^1.6.5",
    "browser-sync": "~2.14.0",
    "del": "^2.2.0",
    "eslint": "^3.1.1",
    "eslint-config-airbnb": "^9.0.1",
    "eslint-plugin-import": "^1.12.0",
    "eslint-plugin-jasmine": "^1.8.1",
    "eslint-plugin-jsx-a11y": "^2.0.1",
    "gh-pages": "^0.11.0",
    "gulp": "^3.9.0",
    "gulp-load-plugins": "^1.1.0",
    "gulp-newer": "~1.1.0",
    "gulp-rename": "^1.2.2",
    "gulp-uglify": "~1.5.4",
    "istanbul-instrumenter-loader": "~0.1.3",
    "jasmine-core": "~2.4.1",
    "karma": "~0.13.15",
    "karma-coverage": "~0.5.3",
    "karma-jasmine": "~0.3.6",
    "karma-phantomjs-launcher": "~0.2.1",
    "karma-sourcemap-loader": "~0.3.6",
    "karma-spec-reporter": "~0.0.23",
    "karma-webpack": "~1.7.0",
    "lodash": "^4.14.2",
    "phantomjs": "~1.9.18",
    "pkgfiles": "^2.3.0",
    "run-sequence": "^1.1.5",
    "vinyl-named": "^1.1.0",
    "webpack": "^1.12.9",
    "webpack-stream": "^3.1.0"
  },
  "scripts": {
    "gh-pages": "npm run build && gh-pages -d dist",
    "register-bower": "bower register slimfit git://github.com/kristw/slimfit",
    "browser-sync": "browser-sync start --server 'dist' --browser \"Google Chrome\" --files \"dist/**/*.*\" --port 7000",
    "webpack": "webpack",
    "webpack-watch": "webpack --watch",
    "test": "karma start --single-run",
    "tdd": "karma start",
    "build": "NODE_ENV=production gulp build",
    "dev": "gulp",
    "start": "NODE_ENV=production gulp",
    "eslint": "eslint --ignore-path .gitignore \"src/**/*.@(js|jsx)\"",
    "eslint-fix": "eslint --fix --ignore-path .gitignore \"src/**/*.@(js|jsx)\"",
    "preversion": "npm test",
    "version": "npm run build && git add -A dist",
    "postversion": "git push ; git push --tags; pkgfiles",
    "prepublish": "pkgfiles"
  },
  "repository": "kristw/slimfit",
  "bugs": {
    "url": "https://github.com/kristw/slimfit/issues"
  }
}
